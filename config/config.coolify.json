{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "restrict_to_workspace": true,
      "model": "openrouter",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "model_list": [],
  "providers": {
    "openrouter": {
      "api_key": "",
      "api_base": "https://openrouter.ai/api/v1"
    }
  },
  "tools": {
    "web": {
      "duckduckgo": { "enabled": true, "max_results": 5 },
      "brave": { "enabled": false },
      "tavily": { "enabled": false }
    }
  },
  "heartbeat": {
    "enabled": true,
    "interval": 60
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}
